#include <stdio.h>

void matrixMultiply(int m, int n, int p, int A[][n], int B[][p], int result[][p]) {
    for(int i=0; i<m; i++) {
        for(int j=0; j<p; j++) {
            result[i][j] = 0;
            for(int k=0; k<n; k++) {
                result[i][j] += A[i][k] * B[k][j];
            }
        }
    }
}

int main() {
    int m=2, n=2, p=2;
    int A[m][n] = { {1, 1}, {2, 2} };
    int B[n][p] = { {1, 1}, {2, 2} };
    int result[m][p];
    
    matrixMultiply(m, n, p, A, B, result);
    
    printf("Result:\n");
    for(int i=0; i<m; i++) {
        for(int j=0; j<p; j++) {
            printf("%d ", result[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
output:
Result:
3 3
6 6
